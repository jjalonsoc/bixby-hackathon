dialog (Result) {
  match {
    Audiobook(this)
  }
  if (size(this) > 1) {
   template ("I found these books in your audio library:")
    {
      speech("I found these books in your audio library... #{value(this.name2)}")
    }   
  }
  else {
      template ("Playing #{value(this.name)}.")
  }

 }